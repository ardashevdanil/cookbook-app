{
  "name": "cookbook-app",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "build": "babel lib -d dist -D",
    "client": "cd lib/client && yarn start",
    "dev": "concurrently --kill-others-on-fail \"PORT=3001 yarn server\" \"yarn client\"",
    "server": "babel-node ./lib/bin/www",
    "start": "cd lib/client && yarn build && cd .. && node ./bin/www",
    "test": "cd lib/client && yarn test"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "react": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.0",
    "@babel/core": "7.1.0",
    "@babel/node": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "concurrently": "4.0.1",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1"
  }
}
